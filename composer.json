{
    "name": "triawarman/yii2-vanillajs",
    "description": "Collection of html-vanilla js components for yii2",
    "keywords": ["yii2", "yii2-extension"],
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Triawarman",
            "email": "3awarman@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/triawarman/yii2-vanillajs/issues?state=open",
        "source": "https://github.com/triawarman/yii2-vanillajs"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "bower-asset/choices.js": "~9.0.0",
        "yiisoft/yii2": "~2.0.14"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "extra": {
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "vendor/assets/{$name}/": ["type:bower-asset", "type:npm-asset"]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "triawarman\\yii2VanillaJs\\": "src"
        }
    }
}
